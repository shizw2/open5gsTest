logger:
  file:
    path: @localstatedir@/log/@project_name@/sacc.log
global:
  parameter:
    capacity: 100
    group: 1
    node: 2
    deviceType: test
    deviceName: test
    deviceSeq: 1
    version: 1.1
  cli:
    server:
      - address: 127.0.0.19
        port: 2300
  deviceName: WJTEST1
sacc:
