    server:
      - address: 127.0.0.19
        port: 2333
    client:
      nrf:
        - uri: http://127.0.0.19:8600
  metrics:
    server:
      - address: 127.0.0.19
        port: 9087
  scanInterval: 5
  heartbeatInterval: 5
  heartbeatLost: 5
  enable: true
  rule: T2
