logger:
  file:
    path: @localstatedir@/log/@project_name@/sacc.log
global:
  parameter:
    capacity: 100
    group: 1
    node: 1
  cli:
    server:
      - address: 127.0.0.18
        port: 2300

sacc:
  sbi:
    server:
      - address: 127.0.0.18
        port: 7777
    client:
      nrf:
        - uri: http://127.0.0.10:7777
#      scp:
#        - uri: http://127.0.0.200:7777
  metrics:
    server:
      - address: 127.0.0.18
        port: 9087
        
  device_type: debug
  device_name: test
  device_seq: 123456789
  version: 1.0      
  role: T2
  node_num: 2
  enable: true
  inherite_enable: true
  scan_interval: 5
  heartbeat_interval: 5
  heartbeat_lost: 5
  
udms:
  - node: udm_1_1
    sbi:
      server:
        - address: 127.0.0.12
          port: 7777
      client:
        nrf:
          - uri: http://127.0.0.10:7777
    info:
      supi:
        - range: 46001-46002
